{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": ["App.tsx"],
  "project": ["src/**/*.{ts,tsx}"],
  "ignore": ["babel.config.js"],
  "ignoreDependencies": ["babel-plugin-module-resolver", "babel-preset-expo"],
  "ignoreBinaries": ["expo-doctor"],
  "ignoreExportsUsedInFile": true,
  "ignoreWorkspaces": [],
  "rules": {
    "exports": "off",
    "types": "off"
  }
}
